        <form >
            <div class="flex items-center w-full">
                <div><input placeholder="What's Next?" required  class="border-2 border-gray-500  m-2 p-2 border-zinc-950 rounded" type="text" /></div>
                <div><input type="date" required class="m-2 border-2 border-gray-500  p-2 rounded"/></div>
                <div><button type="submit" class="m-2  text-white rounded px-5 py-2 bg-green-700">Add</button></div>
            </div>
           </form>


           <div class="p-2">
            <h5 style="display:inline"  class="m-2  text-xl font-bold">Overdue</h5> <span style="display:inline" id='count-overdue' class="bg-gray-200 text-black px-3 py-1 rounded text-sm mr-2 mb-2">
               <%= overdue.length %>
            </span>
                <ul class="list-none">


                    <% for(let i=0;i<overdue.length; i++){ %>

                        <li class="todo-item">
                            <div class="cursor-pointer flex items-center mx-5 my-2 rounded hover:text-black">
                                <input id="<%= overdue[i].id %>" type="checkbox" class="w-4 h-4 text-blue-600 accent-green-600"/>
                                <label for="<%= overdue[i].id %>"  class="cursor-pointer  hover:text-black text-gray-700 text-base ml-2">
                                   <%= overdue[i].title %>
                                </label>
                                <a class=" ml-2 trash-icon hidden"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="#dd0000" class=" bg-red w-4 h-4">
                                    <path fill-rule="evenodd" d="M16.5 4.478v.227a48.816 48.816 0 0 1 3.878.512.75.75 0 1 1-.256 1.478l-.209-.035-1.005 13.07a3 3 0 0 1-2.991 2.77H8.084a3 3 0 0 1-2.991-2.77L4.087 6.66l-.209.035a.75.75 0 0 1-.256-1.478A48.567 48.567 0 0 1 7.5 4.705v-.227c0-1.564 1.213-2.9 2.816-2.951a52.662 52.662 0 0 1 3.369 0c1.603.051 2.815 1.387 2.815 2.951Zm-6.136-1.452a51.196 51.196 0 0 1 3.273 0C14.39 3.05 15 3.684 15 4.478v.113a49.488 49.488 0 0 0-6 0v-.113c0-.794.609-1.428 1.364-1.452Zm-.355 5.945a.75.75 0 1 0-1.5.058l.347 9a.75.75 0 1 0 1.499-.058l-.346-9Zm5.48.058a.75.75 0 1 0-1.498-.058l-.347 9a.75.75 0 0 0 1.5.058l.345-9Z" clip-rule="evenodd" />
                                  </svg>
                                  </a>
                            </div>
                        </li>

                    <% } %>
                    
                </ul>

                <h5 style="display:inline"  class="m-2  text-xl font-bold">Due Today</h5> <span style="display:inline" id='count-due-today' class="bg-gray-200 text-black px-3 py-1 rounded text-sm mr-2 mb-2"> <%= duetoday.length %></span>
                <ul class="list-none">
                    <% for(let i=0;i<duetoday.length; i++){ %>

                        <li class="todo-item">
                            <div class="cursor-pointer flex items-center mx-5 my-2 rounded hover:text-black">
                                <input id="<%= duetoday[i].id %>" type="checkbox" class="w-4 h-4 text-blue-600 accent-green-600"/>
                                <label for="<%= duetoday[i].id %>"  class="cursor-pointer  hover:text-black text-gray-700 text-base ml-2">
                                   <%= duetoday[i].title %>
                                </label>
                                <a class=" ml-2 trash-icon hidden"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="#dd0000" class=" bg-red w-4 h-4">
                                    <path fill-rule="evenodd" d="M16.5 4.478v.227a48.816 48.816 0 0 1 3.878.512.75.75 0 1 1-.256 1.478l-.209-.035-1.005 13.07a3 3 0 0 1-2.991 2.77H8.084a3 3 0 0 1-2.991-2.77L4.087 6.66l-.209.035a.75.75 0 0 1-.256-1.478A48.567 48.567 0 0 1 7.5 4.705v-.227c0-1.564 1.213-2.9 2.816-2.951a52.662 52.662 0 0 1 3.369 0c1.603.051 2.815 1.387 2.815 2.951Zm-6.136-1.452a51.196 51.196 0 0 1 3.273 0C14.39 3.05 15 3.684 15 4.478v.113a49.488 49.488 0 0 0-6 0v-.113c0-.794.609-1.428 1.364-1.452Zm-.355 5.945a.75.75 0 1 0-1.5.058l.347 9a.75.75 0 1 0 1.499-.058l-.346-9Zm5.48.058a.75.75 0 1 0-1.498-.058l-.347 9a.75.75 0 0 0 1.5.058l.345-9Z" clip-rule="evenodd" />
                                  </svg>
                                  </a>
                            </div>
                        </li>

                    <% } %>
                    
                </ul>
                <h5 style="display:inline"  class="m-2  text-xl font-bold">Due Later</h5> <span style="display:inline" id='count-due-later' class="bg-gray-200 text-black px-3 py-1 rounded text-sm mr-2 mb-2"> <%= duelater.length %></span>
                <ul class="list-none">
                    <% for(let i=0;i<duelater.length; i++){ %>

                        <li class="todo-item">
                            <div class="cursor-pointer flex items-center mx-5 my-2 rounded hover:text-black">
                                <input id="<%= duelater[i].id %>" type="checkbox" class="w-4 h-4 text-blue-600 accent-green-600"/>
                                <label for="<%= duelater[i].id %>"  class="cursor-pointer  hover:text-black text-gray-700 text-base ml-2">
